# Production environment optimizations for memory usage
NODE_ENV=production

# Disable resource-intensive features
NEXT_TELEMETRY_DISABLED=1
SKIP_ENV_VALIDATION=1
DISABLE_ESLINT=true

# Disable analytics and monitoring to save memory
NEXT_PUBLIC_ANALYTICS_POSTHOG=
NEXT_PUBLIC_ANALYTICS_UMAMI=
NEXT_PUBLIC_SENTRY_DSN=
ENABLE_SENTRY=false
ENABLE_LANGFUSE=false

# Service mode
NEXT_PUBLIC_SERVICE_MODE=server

# Minimize build features
MINIMIZE_MEMORY_USAGE=true
